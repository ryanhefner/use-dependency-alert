{
  "name": "use-dependency-alert",
  "version": "0.1.1",
  "description": "React hook for debugging hook dependency arrays to evaluate if they are triggering the hook too often, or not.",
  "repository": {
    "type": "git",
    "url": "ryanhefner/use-dependency-alert"
  },
  "author": "Ryan Hefner <hi@ryanhefner.com> (https://www.ryanhefner.com)",
  "license": "MIT",
  "type": "module",
  "files": [
    "es",
    "src",
    "types",
    "umd",
    "index.js"
  ],
  "directories": {
    "lib": "/src"
  },
  "main": "index.js",
  "module": "es/index.js",
  "jsnext:main": "src/index.js",
  "types": "types",
  "scripts": {
    "clean": "rimraf index.js es types umd",
    "prebuild": "npm run clean",
    "build": "node ./tools/build.js && yarn types",
